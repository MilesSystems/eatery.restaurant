        <script>
            Carbon(() => {

                let $menu = $('li');

                let activity = function () {
                    $("li a").filter(function () {
                        $menu.removeClass('active');
                        return this.href === location.href.replace(/#.*/, "");
                    }).parent().addClass("active");
                };

                activity();

                $menu.click(function () {
                    $menu.removeClass('active');
                    $(this).addClass('active');
                });

                $('#mytitle').click(function () {
                    $menu.removeClass('active');
                });
            })
        </script>

